@model EmployeeViewModel
@{
    ViewData["Title"] = "Edit";
}

<h1 style="text-align:center;">Edit Of Employee</h1>


<form asp-action="Edit" enctype="multipart/form-data">
    <partial name="PartialViews/EditCreatePartialView" model="Model" />
    <input hidden asp-for="ImageName" />
    <input type="submit" value="Save" class=" btn btn-primary" />
    <a asp-action="Index" class=" btn btn-light" style="margin-left: 10px;">Back To List</a>
</form>
@section CSV {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}
