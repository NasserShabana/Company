@model Department
@{
    ViewData["Title"] = "Details";
}
<br />
<br />

<h1 style="text-align:center;">Details Of Department </h1>

<br />
<br />

<div>
    <div class="form-group">
        <label asp-for="Code" class="col-form-label"></label>
        <input asp-for="Code " class=" form-control" readonly />

    </div> 
    <br />
    <div class="form-group">
        <label asp-for="Name" class="col-form-label"></label>
        <input asp-for="Name " class=" form-control" readonly />

    </div> 
    <br />
    <div class="form-group">
        <label asp-for="DateOfCreation" class="col-form-label"></label>
        <input asp-for="DateOfCreation " class=" form-control" readonly />

    </div>
    <br />
    <br />

    <a asp-action="Edit" asp-route-id ="@Model.Id" class="btn btn-warning"> Edit</a>
    <a asp-action="Index" class="btn btn-light" style="margin-left: 10px;"> Back To List</a>

</div>