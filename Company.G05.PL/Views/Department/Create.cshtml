@model Department

@{
    ViewData["Title"] = "Create";
}
<br />
<br />
<h1 style="text-align:center;">Create Of Department </h1>
<br />
<br/>

<form asp-action="Create">

    <div class=" form-group">
        <label asp-for="Code" class="col-form-label"> </label>
        <input asp-for="Code" class=" form-control" />
        <span asp-validation-for="Code " class="text-danger"></span>
    </div> 
    <br />
    <div class=" form-group">
        <label asp-for="Name" class="col-form-label"> </label>
        <input asp-for="Name" class=" form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>

    </div>
    <br />
    <div class=" form-group">
        <label asp-for="DateOfCreation" class="col-form-label"></label>
        <input asp-for ="DateOfCreation" class=" form-control" />
    </div>
<br />
 <br />

<input type="submit" value="Create" class=" btn btn-primary"  />
    <a asp-action="Index" class="btn btn-light" style="margin-left: 10px;"> Back To List</a>

</form>

